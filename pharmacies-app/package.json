{
    "name": "pharmacies-app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host 0.0.0.0",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} .prettierrc.cjs",
        "openapi-generate:ui": "rimraf ./src/apiClients/PharmaciesAPI && npx @openapitools/openapi-generator-cli generate --generator-key ui"
    },
    "dependencies": {
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@mui/material": "^6.1.1",
        "@openapitools/openapi-generator-cli": "^2.13.12",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.11.1",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.0",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "globals": "^15.9.0",
        "prettier": "^3.3.3",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.8.0",
        "vite": "^5.4.1"
    }
}
